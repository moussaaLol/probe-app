{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/search.js"
    },
    {
      "source": "/browse/app", 
      "destination": "/api/app/[id].js"
    }
  ],
  "cleanUrls": true,
  // ADD THIS BLOCK to ensure Vercel includes the template file with the function
  "functions": {
    "api/app/[id].js": {
      "includeFiles": "template/browse/app.html"
    }
  }
}

{
  "outputDirectory": ".",
  "redirects": [
    {
      "source": "/browse/app", 
      "destination": "/api/app/[id].js",
      "permanent": false
    },
    {
      "source": "/browse/app?id=:id", 
      "destination": "/api/app/[id].js?id=:id",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/search.js"
    }
  ],
  "cleanUrls": true,
  "functions": {
    "api/app/[id].js": {
      "includeFiles": "template/browse/app.html"
    }
  }
}

{
  "outputDirectory": ".",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/browse/app",
      "destination": "/api/app/[id]"
    },
    {
      "source": "/browse/app/:id",
      "destination": "/api/app/[id]"
    }
  ],
  "functions": {
    "api/app/[id].js": {
      "includeFiles": "template/browse/app.html"
    }
  }
}

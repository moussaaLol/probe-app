{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/search.js"
    },
    {
      "source": "/browse/app(.*)",
      "destination": "/api/app/[id].js$1"
    }
  ],
  "cleanUrls": true,
  "functions": {
    "api/app/[id].js": {
      "includeFiles": "template/browse/app.html"
    }
  }
}
